@model CreateTeamMemberVM

<div class="col-md-6 grid-margin stretch-card">
<div class="card">
    <div class="card-body">

        <form class="forms-sample" method="post" action="create" enctype="multipart/form-data">
            <div class="form-group">
                <label asp-for="Name"></label>
                <input asp-for="Name" class="form-control" placeholder="TeamMember Name">
                <span class="text text-danger" asp-validation-for="Name"></span>

                <label asp-for="Profession"></label>
                <input asp-for="Profession" class="form-control" placeholder="TeamMember Profession">
                <span class="text text-danger" asp-validation-for="Profession"></span>

                <label asp-for="Photo"></label>
                <input asp-for="Photo" class="form-control" placeholder="TeamMember Photo">
                <span class="text text-danger" asp-validation-for="Photo"></span>
            </div>
            <button type="submit" class="btn btn-primary me-2 text-light">Create</button>
            <button class="btn btn-light" asp-action="Index">Go back</button>

        </form>
    </div>
</div>
</div>

